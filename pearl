def randomlogo():
        listlogo = [ban1,ban2,ban3,ban4,ban5,ban6]
        logo=(random.choice(listlogo))
        sprint(logo)
randomlogo()
sprint(banner)
def main():
  	 i = input(G + "[pearl~>")
  	 if i == 'msf':
  	 	print("Opening Metasploit Automater")
  	 	time.sleep(2)
  	 	os.system("cd /data/data/com.termux/files/home/Blackpearl-framework/lib;python msf.py")
  	 elif i == 'nmap':
  	 	print("Opening Network Mapper Automater")
  	 	time.sleep(2)
  	 	os.system("cd /data/data/com.termux/files/home/Blackpearl-framework/lib;bash nmapper.sh")
  	 elif i == 'hydra':
  	 	print("Opening Hydra Automater")
  	 	time.sleep(2)
  	 	os.system("cd /data/data/com.termux/files/home/Blackpearl-framework/lib;python hydra.py")
  	 elif i == 'wl':
  	 	print("Opening Wordlist Automater")
  	 	time.sleep(2)
  	 	os.system("cd /data/data/com.termux/files/home/Blackpearl-framework/module;bash wl.sh")
  	 elif i == 'ddos':
  	 	print("Opening DDOS Automater")
  	 	time.sleep(2)
  	 	os.system("cd /data/data/com.termux/files/home/Blackpearl-framework/lib;python Ddos.py")
  	 elif i == 'help':
  	 	print("Opening Metasploit Automater")
  	 elif i == 'exit':
 	  	sys.exit(1)
  	 elif i == 'about':
  	 	os.system("cd /data/data/com.termux/files/home/Blackpearl-framework/module;python about.py")
  	 elif i == 'clear':
  	 	os.system("clear")
  	 elif i == 'update':
  	 	os.system("cd /data/data/com.termux/files/home/Blackpearl-framework/module;python update.py")
  	 elif i == 'sql':
  	 	print("Opening Sqlmap Automater")
  	 	os.system("cd /data/data/com.termux/files/home/Blackpearl-framework/lib;python sql.py")
  	 else:
  	 	print("[!] Invalid Inputs")
  	 	sys.exit(2)
main()
   
